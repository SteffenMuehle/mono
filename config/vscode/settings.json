{
    "jupyter.widgetScriptSources": [
        "jsdelivr.com",
        "unpkg.com"
    ],
    "workbench.tree.indent": 15,
    "editor.fontSize": 13,
    "[python]": {
        "editor.formatOnType": true
    },
    "editor.accessibilitySupport": "off",
    "python.terminal.activateEnvironment": false,
    "terminal.integrated.fontSize": 12,
    "editor.cursorStyle": "line",
    "editor.cursorWidth": 4,
    "window.clickThroughInactive": false,
    "notebook.consolidatedRunButton": true,
    "workbench.colorTheme": "Default High Contrast",
    "workbench.colorCustomizations": {
        "titleBar.activeForeground": "#ffffff", 
        "tab.activeBackground": "#d35f00", 
        "tab.unfocusedActiveBackground": "#008493",
        "tab.activeForeground": "#ffffff",
        "tab.unfocusedActiveForeground": "#ffffff",
        "tab.inactiveBackground": "#004249",
        "tab.inactiveForeground": "#ffffff",
        "terminal.background" : "#002626",
        "sideBar.background": "#00122d",
        "activityBar.background": "#232323",
        "contrastBorder": "#ad4e00",
        "button.background": "#0082b9",
        "button.foreground": "#ffffff",
        "scrollbarSlider.background": "#ababab",
        "minimap.background": "#1b1b1b",
        "editorCursor.foreground": "#ffffff",
        "minimap.findMatchHighlight": "#09bf00",
        "editor.findMatchBackground": "#00ff00",
        "editor.findMatchBorder": "#00ff00",
        "editorWidget.background": "#078700",
        "editorHoverWidget.background": "#22343f",
        "editorRuler.foreground": "#00484b",
        "editorInlayHint.foreground": "#707070",
        "editorInlayHint.background": "#2a2a2a",
    },
    "jupyter.askForKernelRestart": false,
    "workbench.iconTheme": "vscode-icons-mac",
    "gitlens.ai.experimental.provider": "openai",
    "gitlens.ai.experimental.openai.model": "gpt-4",
    "diffEditor.ignoreTrimWhitespace": false,
    "editor.rulers": [
        125
    ],
    "terminal.external.linuxExec": "iTerm2",
    "rust-analyzer.diagnostics.experimental.enable": true,
    "rust-analyzer.diagnostics.warningsAsHint": ["dead_code", "unused_variables"],
    "rust-analyzer.inlayHints.bindingModeHints.enable": true,
    "rust-analyzer.inlayHints.closureReturnTypeHints.enable": "with_block",
    "rust-analyzer.inlayHints.lifetimeElisionHints.enable": "skip_trivial",
    "rust-analyzer.inlayHints.typeHints.hideClosureInitialization": true,
    "rust-analyzer.procMacro.enable": true,
    "rust-analyzer.workspace.symbol.search.kind": "all_symbols",
    "[rust]": {
    "editor.formatOnSave": true
    },
    "explorer.confirmDelete": false,
    "files.exclude": {
        "**/.git": false
    },
}